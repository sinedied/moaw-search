{
  "containers": [
    {
      "name": "search-api",
      "options": {
        "ingress": {
          "external": true,
          "targetPort": 8080
        },
        "scale": {
          "minReplicas": 1
        }
      }
    },
    {
      "name": "qdrant",
      "options": {
        "image": "qdrant/qdrant",
        "ingress": {
          "external": true,
          "targetPort": 6333
        },
        "scale": {
          "minReplicas": 1
        }
      }
    }
  ],
  "websites": [
    {
      "name": "search-ui"
    }
  ],
  "openai": {
    "customSubDomainName": "moaw",
    "deployments": [
      {
        "name": "text-embedding-ada-002",
        "version": "2"
      },
      {
        "name": "gpt-35-turbo",
        "version": "0301"
      }
    ]
  }
}
