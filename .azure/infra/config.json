{
  "containers": [
    {
      "name": "search-api",
      "options": {
        "ingress": {
          "external": true,
          "targetPort": 8080
        },
        "scale": {
          "minReplicas": 1
        }
      }
    },
    {
      "name": "qdrant",
      "options": {
        "image": "qdrant/qdrant",
        "ingress": {
          "external": true,
          "targetPort": 6333,
          "allowInsecure": true
        },
        "scale": {
          "minReplicas": 1,
          "resources": {
            "cpu": 1,
            "memory": "2Gi"
          }
        },
        "volumeMounts": [
          {
            "storageName": "qdrantfiles",
            "volumeName": "qdrantstorage",
            "mountPath": "/qdrant/storage"
          }
        ]
      }
    }
  ],
  "websites": [
    {
      "name": "search-ui"
    }
  ],
  "openai": {
    "customSubDomainName": "moaw",
    "models": [
      {
        "name": "text-embedding-ada-002",
        "version": "2"
      },
      {
        "name": "gpt-35-turbo",
        "version": "0301"
      }
    ]
  },
  "storage": {
    "fileShares": ["qdrantfiles"]
  }
}
